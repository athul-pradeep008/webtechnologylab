  <!-- Order Form -->
  <div class="order-section">
    <h2>Order Cake</h2>
    <label>Rainbow Cake (Quantity)</label><br>
    <input type="number" id="rainbow" min="0" value="0"><br>

    <label>Chocolate Cake (Quantity)</label><br>
    <input type="number" id="choco" min="0" value="0"><br>

    <label>Red Velvet Cake (Quantity)</label><br>
    <input type="number" id="velvet" min="0" value="0"><br>

    <label>Black Forest Cake (Quantity)</label><br>
    <input type="number" id="forest" min="0" value="0"><br>

    <button id="orderBtn">Place Order</button>

    <div class="total-section">
      <p id="message"></p>
      <h3 id="totalPrice"></h3>
    </div>
  </div>

  <script>
    $(document).ready(function() {
      $("#orderBtn").click(function() {
        let rainbow = parseInt($("#rainbow").val()) || 0;
        let choco = parseInt($("#choco").val()) || 0;
        let velvet = parseInt($("#velvet").val()) || 0;
        let forest = parseInt($("#forest").val()) || 0;

        // Prices of cakes
        let rainbowPrice = 300;
        let chocoPrice = 200;
        let velvetPrice = 250;
        let forestPrice = 350;

        // Calculate total
        let total = (rainbow * rainbowPrice) + 
                    (choco * chocoPrice) + 
                    (velvet * velvetPrice) + 
                    (forest * forestPrice);

        if (total === 0) {
          $("#message").html("⚠️ Please enter at least one cake quantity!");
          $("#totalPrice").html("");
        } else {
          $("#message").html("✅ Purchase Order Bill!!!");
          $("#totalPrice").html("Total Price: Rs. " + total);
        }
      });
    });
  </script>

